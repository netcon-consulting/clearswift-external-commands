{
    "Check private": {
        "media_types": {
            "SMTP": [ "not_protected" ]
        },
        "parameters": "-t \"%ITEMTYPE%\" -c \"Config - Check private\" %FILENAME% %LOGNAME% \"External command - check_private\"",
        "responses": {
            "NONE": "Private",
            "DETECTED": "Not private",
            "MODIFIED": "Invalid private (%LOGTEXT%)",
            "NOT_CHECKED": "Error (%LOGTEXT%)"
        },
        "disposal_actions": {
            "detected": { "primary": "hold:Not private" },
            "modified": { "primary": "hold:Invalid private" }
        },
        "config": {
            "max_size_kb": { "type": "integer", "description": "maximum mail size for private mails in kB", "value": "25" }
        }
    }
}
