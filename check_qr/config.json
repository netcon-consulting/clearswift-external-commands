{
    "Check QR": {
        "packages": [
            "zbar"
        ],
        "modules": [
            "Pillow",
            "pyzbar"
        ],
        "list_url": [
            "URL blacklist",
            "URL whitelist"
        ],
        "media_types": {
            "GIF": [ ],
            "JPEG": [ ],
            "BMP": [ ],
            "PNG": [ ],
            "TIFF": [ ]
        },
        "responses": {
            "NONE": "No QR-code with URL or URLs not malicious",
            "DETECTED": "Malicious URL (%LOGTEXT%)",
            "NOT_CHECKED": "Error (%LOGTEXT%)"
        },
        "disposal_actions": {
            "detected": { "primary": "hold:Check QR" }
        },
        "config": {
            "name_url_blacklist": { "type": "string", "description": "name of URL blacklist", "value": "\"URL blacklist\"" },
            "name_url_whitelist": { "type": "string", "description": "name of URL whitelist", "value": "\"URL whitelist\"" }
        }
    }
}
