{
    "Tag mail": {
        "list_address": [
            "Internal Domains"
        ],
        "media_types": {
            "OUTLOOK": [ "not_protected" ],
            "TNEF": [ "not_protected" ],
            "SMTP": [ "not_protected" ]
        },
        "return_codes": {
            "0": { "action": "NONE", "description": "Not modified" },
            "1": { "action": "MODIFIED", "description": "Mail tagged" },
            "99": { "action": "NOT_CHECKED", "description": "Error (%LOGTEXT%)" }
        },
        "config": {
            "address_tag": { "type": "string", "description": "", "value": "\"[EXT]\"" },
            "name_domain_list": { "type": "string", "description": "", "value": "\"Internal Domains\"" },
            "clean_text": { "type": "boolean", "description": "", "value": "true" },
            "clean_html": { "type": "boolean", "description": "", "value": "true" },
            "subject_tag": { "type": "string", "description": "", "value": "\"[EXTERNAL]\"" },
            "text_tag": { "type": "string", "description": "", "value": "\"###### EXTERNAL SENDER ######\\n\"" },
            "text_top": { "type": "boolean", "description": "", "value": "true" },
            "html_tag": { "type": "string", "description": "", "value": "\"###### EXTERNAL SENDER ######\\n\"" },
            "html_top": { "type": "boolean", "description": "", "value": "true" },
            "html_tag_id": { "type": "string", "description": "", "value": "\"tag_mail_1\"" }
        }
    },
    "Un-tag mail": {
        "list_address": [
            "Internal Domains"
        ],
        "media_types": {
            "OUTLOOK": [ "not_protected" ],
            "TNEF": [ "not_protected" ],
            "SMTP": [ "not_protected" ]
        },
        "parameters": "-r %FILENAME% %LOGNAME% 'Config - Tag mail'",
        "return_codes": {
            "0": { "action": "NONE", "description": "Not modified" },
            "1": { "action": "MODIFIED", "description": "Mail un-tagged" },
            "99": { "action": "NOT_CHECKED", "description": "Error (%LOGTEXT%)" }
        }
    }
}
